{
    "name": "Getting Started Example",
    "permissions": ["activeTab"],
    "version": "1.0",
    "description": "Build an Extension!",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["filters.js"]
      }
    ],
    "browser_action": {
      "default_icon": "superposition.png",
      "default_popup": "popup.html",
      "default_title": "Check to see if our code is on this page"
    },
    "manifest_version": 2
  }
